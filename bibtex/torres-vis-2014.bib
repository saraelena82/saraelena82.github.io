@ARTICLE{torres2014vis,
  author = {Torres-Guerrero, Francisco and Sara E. Garza},
  title = {{Visualización de Redes Semánticas Naturales a través de Acomodo
	de Grafos Dirigido por Fuerzas}},
  journal = {Daena: International Journal of Good Conscience},
  year = {2014},
  volume = {9},
  pages = {68-75},
  number = {3},
  abstract = {En la presente investigación se realiza un estudio exploratorio con
	el objetivo de realizar un análisis para la creación de una visualización
	de una red semántica natural de la temática de ecología. Se realizaron
	tres diferentes visualizaciones utilizando las aplicaciones Knot
	(para la visualización de Pathfinder), Wordle (para la visualización
	de Tag Cloud) y Smart Semantic Cloud (para la visualización de Semantic
	Cloud). Este último método es el que proponemos.}
}
